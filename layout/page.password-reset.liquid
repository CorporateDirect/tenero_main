  {% case template.name %}
  {% when 'product' %}
    <script type="application/ld+json">
      {{ product | structured_data }}
    </script>
  {% when 'article' %}
    <script type="application/ld+json">
      {{ article | structured_data }}
    </script>
  {% when 'collection' %}
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "WebPage",
        "url": "{{ shop.url }}",
        "mainEntity": {
          "@type": "OfferCatalog",
          "name": {{ collection.title | json }},
          "url": "{{ collection.url }}",
          "numberOfItems": {{ collection.products.size }}
        }
      }
    </script>
{% endcase %}
{% assign min_price_cents = nil %}
{% assign max_price_cents = 0 %}
{% for product in collections.all.products %}
  {% for variant in product.variants %}
    {% if min_price_cents == nil or variant.price < min_price_cents %}
      {% assign min_price_cents = variant.price %}
    {% endif %}
    {% if variant.price > max_price_cents %}
      {% assign max_price_cents = variant.price %}
    {% endif %}
  {% endfor %}
{% endfor %}
<script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "LocalBusiness",
    "mainEntityOfPage": { "@type": "WebPage", "@id": "{{ shop.url }}" },
    "name": {{ shop.name | json }},
    "telephone": {{ shop.phone | json }},
    "email": {{ shop.email | json }},
    "priceRange": {{ min_price_cents | money | append: " - " | append: (max_price_cents | money) | json }},
    "address": {
      "@type": "PostalAddress",
      "streetAddress": {{ shop.address.address1 | json }},
      "addressLocality": {{ shop.address.city | json }},
      "addressRegion": {{ shop.address.province | json }},
      "addressCountry": {{ shop.address.country_code | json }},
      "postalCode": {{ shop.address.zip | json }}
    },
    "url": "{{ shop.url }}"
  }
</script>
