  <script>
(() => {
  let splitWords = null;
  let splitLines = null;
  const ABOVE_FOLD_RATIO = 0.80;  // includes hero paragraph under H1
  const TAG_PREFIX = "tk-";
  const isAboveFold = (el) => el.getBoundingClientRect().top < window.innerHeight * ABOVE_FOLD_RATIO;
  const killTaggedScrollTriggers = () => {
    if (!window.ScrollTrigger) return;
    ScrollTrigger.getAll().forEach(st => {
      if (st?.vars?.id && st.vars.id.startsWith(TAG_PREFIX)) st.kill();
    });
  };
  const init = () => {
    if (!window.gsap || !window.ScrollTrigger || !window.SplitType) return;
    gsap.registerPlugin(ScrollTrigger);
    // clean previous run
    killTaggedScrollTriggers();
    if (splitWords) splitWords.revert();
    if (splitLines) splitLines.revert();
    // split ONLY what needs it
    splitWords = new SplitType("[scrub-each-word][text-split]", { types: "words", tagName: "span" });
    splitLines = new SplitType("[lines-reveal][text-split]", { types: "lines", tagName: "span" });
    // reveal after split (prevents &ldquo;animated while hidden&rdquo; above-fold issue)
    gsap.set("[text-split]", { opacity: 1 });
    // ---- WORD SCRUB ----
    document.querySelectorAll("[scrub-each-word][text-split]").forEach((el, idx) => {
      const words = el.querySelectorAll(".word");
      if (!words.length) return;
      const aboveFold = isAboveFold(el);
      // optional wrapper trigger
      const wrapperTrigger = el.closest("[scrub-trigger]");
      const triggerEl = wrapperTrigger || el;
      // baseline
      gsap.set(words, { opacity: 0.2, clearProps: "transform" });
      if (aboveFold) {
        // play once on load
        gsap.to(words, {
          opacity: 1,
          duration: 0.6,
          ease: "power1.out",
          stagger: { each: 0.06 }
        });
      } else {
        // IMPORTANT: give staggered scrub enough scroll distance
        const endPx = Math.max(240, words.length * 30); // adjust multiplier if you want longer/shorter
        gsap.to(words, {
          opacity: 1,
          ease: "none",
          stagger: { each: 0.06 },
          scrollTrigger: {
            id: `${TAG_PREFIX}scrub-${idx}`,
            trigger: triggerEl,
            start: "top 90%",
            end: `+=${endPx}`,
            scrub: true,
            invalidateOnRefresh: true
          }
        });
      }
    });
    // ---- LINE REVEAL (NO scrub) ----
    document.querySelectorAll("[lines-reveal][text-split]").forEach((el, idx) => {
      const lines = el.querySelectorAll(".line");
      if (!lines.length) return;
      const aboveFold = isAboveFold(el);
      // NOTE: if SplitType detects only 1 line, it will animate as one piece (expected).
      const anim = {
        opacity: 0,
        y: "0.6em",
        duration: 0.6,
        ease: "power2.out",
        stagger: 0.12,
        clearProps: "transform"
      };
      if (aboveFold) {
        gsap.from(lines, anim);
      } else {
        gsap.from(lines, {
          ...anim,
          scrollTrigger: {
            id: `${TAG_PREFIX}lines-${idx}`,
            trigger: el,
            start: "top 85%",
            once: true,
            invalidateOnRefresh: true
          }
        });
      }
    });
    ScrollTrigger.refresh();
  };
  const initWhenReady = () => {
    init();
    // fonts can change line breaks + trigger positions
    if (document.fonts && document.fonts.ready) {
      document.fonts.ready.then(() => init());
    }
  };
  window.addEventListener("DOMContentLoaded", initWhenReady);
  window.addEventListener("load", initWhenReady);
  let t;
  window.addEventListener("resize", () => {
    clearTimeout(t);
    t = setTimeout(initWhenReady, 200);
  });
})();
</script>
