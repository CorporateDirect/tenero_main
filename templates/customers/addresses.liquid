                            <div class="address_container is-header">
                  {% if address == customer.default_address %}    <div li-if="address == customer.default_address" class="text-size-small">
    MainÂ Address    </div>
{% endif %}
                  {% if address != customer.default_address %}    <div li-if="address != customer.default_address" class="text-size-small">
    Other Address    </div>
{% endif %}
                </div>
                            
                
                            <div class="address_container">
                  <div li-object="address | format_address" class="text-size-small">{{ address | format_address }}</div>
                  <div class="addresses_edit">
